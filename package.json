{
  "name": "sistema-command-d",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc -b && vite build",
    "start": "node server/node-build.js",
    "typecheck": "tsc -b --noEmit",
    "test": "vitest",
    "test:setup": "tsx tests/setup/auth.setup.ts",
    "test:setup:owner": "tsx tests/setup/auth.owner.setup.ts",
    "test:setup:employee": "tsx tests/setup/auth.employee.setup.ts",
    "test:setup:client": "tsx tests/setup/auth.client.setup.ts",
    "test:setup:all": "npm run test:setup:owner && npm run test:setup:employee && npm run test:setup:client",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:complete": "playwright test tests/e2e/complete-system.spec.ts",
    "test:e2e:complete:headed": "playwright test tests/e2e/complete-system.spec.ts --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:ui": "playwright test --ui",
    "qa": "npm run test:setup && npm run test:e2e",
    "qa:headed": "npm run test:setup && npm run test:e2e:headed",
    "qa3": "npm run test:setup:all && playwright test",
    "qa3:headed": "npm run test:setup:all && playwright test --headed",
    "autofix:analyze": "tsx tests/autofix/run-autofix.ts",
    "autofix:ui": "tsx scripts/autofix/ui-fixes.ts",
    "bot": "tsx tests/bot/run-bot.ts once",
    "bot:watch": "tsx tests/bot/run-bot.ts watch",
    "bot:quick": "tsx tests/bot/run-bot.ts quick",
    "bot:full": "tsx scripts/run-bot-with-server.ts",
    "bot:complete": "tsx tests/bot/complete-test-bot.ts",
    "bot:auto-heal": "tsx tests/bot/auto-healing-bot.ts",
    "bot:intelligent": "tsx tests/bot/auto-healing-bot.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
